# https://www.mycli.net/config
[main]
smart_completion = True
multi_line = True
destructive_warning = True
timing = True
# fancy_grid, grid, html, latex, latex_booktabs, mediawiki, orgtbl, pipe, plain, psql, rst, simple, tsv
# Some examples:
# https://bitbucket.org/astanin/python-tabulate#rst-header-table-format
