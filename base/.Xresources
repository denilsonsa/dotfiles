Xcursor.theme:	crystalblue
Xcursor.size:	18

! See also: /etc/X11/app-defaults/XScreenSaver*
xscreensaver.Dialog.headingFont:	-*-terminus-bold-r-*-*-*-180-*-*-*-*-iso8859-1
xscreensaver.Dialog.bodyFont:	-*-terminus-bold-r-*-*-*-140-*-*-*-*-iso8859-1
xscreensaver.Dialog.labelFont:	-*-terminus-bold-r-*-*-*-140-*-*-*-*-iso8859-1
xscreensaver.Dialog.unameFont:	-*-terminus-bold-r-*-*-*-120-*-*-*-*-iso8859-1
xscreensaver.Dialog.buttonFont:	-*-terminus-bold-r-*-*-*-140-*-*-*-*-iso8859-1
xscreensaver.Dialog.dateFont:	-*-terminus-medium-r-*-*-*-120-*-*-*-*-iso8859-1
xscreensaver.passwd.passwdFont:	-*-terminus-medium-r-*-*-*-140-*-*-*-iso8859-1

xscreensaver.Dialog.foreground:	#000000
xscreensaver.Dialog.background:	#C0C0C0
xscreensaver.Dialog.Button.foreground:	#000000
xscreensaver.Dialog.Button.background:	#CCCCCC
xscreensaver.Dialog.text.foreground:	#000000
xscreensaver.Dialog.text.background:	#FFFFFF
xscreensaver.Dialog.topShadowColor:	#E0E0E0
xscreensaver.Dialog.bottomShadowColor:	#A0A0A0
! Small dimensions for the logo are ignored anyway.
xscreensaver.Dialog.logo.width:	200
xscreensaver.Dialog.logo.height:	200
! This is actually the padding.
xscreensaver.Dialog.internalBorderWidth:	16
! Outer dialog border.
xscreensaver.Dialog.borderWidth:	0
! Overall 3D-effect border.
xscreensaver.Dialog.shadowThickness:	2
! Disabling the "New Login" button.
xscreensaver.newLoginCommand:	

